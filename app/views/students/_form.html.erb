<% if flash[:my_errors] %>
  <% flash[:my_errors].each do |error| %>
    <p><%= error %></p>
  <% end %>
<% end %>

<%= form_for @student do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name, id: "test" %>
    <%= f.label :mod %>
    <%= f.number_field :mod %>
    <%= f.submit %>
  
  <% end %>