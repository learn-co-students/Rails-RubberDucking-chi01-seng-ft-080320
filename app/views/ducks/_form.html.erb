<% if flash[:my_errors] %>
  <% flash[:my_errors].each do |error| %>
    <p><%= error %></p>
  <% end %>
<% end %>

<%= form_for @duck do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name, id: "test" %>
    <%= f.label :description %>
    <%= f.text_field :description %>
    <%= f.collection_select(:student_id, @students, :id, :name) %><br /><br />
    <%= f.submit %>
  <% end %>